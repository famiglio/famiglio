{
  "name": "@famiglio/core",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@famiglio/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "nx": {
    "name": "core",
    "tags": [
      "type:lib",
      "scope:shared"
    ],
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "inputs": [
          "production"
        ],
        "outputs": [
          "{projectRoot}/dist"
        ],
        "options": {
          "cwd": "{projectRoot}",
          "parallel": false,
          "commands": [
            "tsdown",
            "tsc -p tsconfig.lib.json"
          ]
        }
      },
      "watch": {
        "executor": "nx:run-commands",
        "inputs": [
          "default"
        ],
        "options": {
          "cwd": "{projectRoot}",
          "commands": [
            "tsdown --watch"
          ]
        }
      }
    }
  }
}
